{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "",
  "main": "build/server.js",
  "type": "module",
  "scripts": {
    "start": "npx nodemon --watch \"src/ssr\" -e ts,tsx --exec \"npm run build:server && node build/server.js\"",
    "build:server": "npx webpack --config ./config/webpack.dev.js --env IS_LOCAL=true --env IS_SERVER=true --env WATCH=false --env ENV=development",
    "build:server:prod-local": "npx webpack --config ./config/webpack.prod.js --env IS_LOCAL=true --env IS_SERVER=true --env WATCH=false  --env ENV=production",
    "run:prod": "nodemon --watch \"src/ssr\" -e ts,tsx --exec \"npm run build:server:prod-local && node build/server.js\"",
    "build:dev": "node config/build.js IS_LOCAL=false ENV=development",
    "start:test-api": "nodemon --watch build/testApi.js build/testApi.js",
    "build:client:prod": "npx webpack --config ./config/webpack.prod.js --env IS_LOCAL=false --env IS_SERVER=false --env ENV=production",
    "build:server:prod": "npx webpack --config ./config/webpack.prod.js --env IS_LOCAL=false --env IS_SERVER=true --env ENV=production",
    "build:prod": "npm run build:server:prod && npm run build:client:prod",
    "build:prod:action": "cross-env GITHUB_ACTION=true npm run build:prod",
    "start:prod": "pm2 start ecosystem.config.cjs --env production",
    "sass:compile": "node-sass src/assets/css/main-style.scss -o src/assets/css && node-sass src/assets/css/main-style.scss -o src/assets/css -w src/assets",
    "sass:compile:win": "node-sass src/assets/css/main-style.scss -o src/assets/css -w src/assets",
    "start:html": "nodemon --watch html html/index.js",
    "run:html": "npm run sass:compile & npm run start:html",
    "test": "jest"
  },
  "author": "Sandip",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^17.0.41",
    "bootstrap": "^5.1.3",
    "dotenv": "^16.0.1",
